    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.saltiresable.charsheet",
      "supportsTablet": true
    },
    "android": {
      "package": "com.saltiresable.charsheet"
    }
  }
}
